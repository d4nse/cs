        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PART2"
        mc:Ignorable="d"
        Title="MainWindow" Height="227" Width="257">
    <Grid>
        <StackPanel>
            <Button x:Name="dancingProgBarsBtn" Content="Dancing Prog Bars" Click="dancingProgBarsBtn_Click" Height="31"/>
            <Button x:Name="barRacingBtn" Content="Bar Racing" Height="31" Click="barRacingBtn_Click"/>
            <Button x:Name="wordCounterBtn" Content="WordCounter" Height="31" Click="wordCounterBtn_Click"/>
        </StackPanel>
    </Grid>
</Window>
