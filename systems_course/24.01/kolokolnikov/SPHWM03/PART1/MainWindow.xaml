<Window x:Class="PART1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PART1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="325">
    <Grid>
        <Label Content="Lower bound" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="80" Height="25"/>
        <Label Content="Upper bound" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Width="80" Height="25"/>
        <TextBox x:Name="lowerBoundTextBox" HorizontalAlignment="Left" Margin="95,17,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="50" Height="20"/>
        <TextBox x:Name="upperBoundTextBox" HorizontalAlignment="Left" Margin="95,42,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="50" Height="20"/>
        <Button x:Name="primeGenStartBtn" Content="Start" HorizontalAlignment="Left" Margin="16,98,0,0" VerticalAlignment="Top" Width="50" Click="primeGenStartBtn_Click" Height="20"/>
        <Label Content="Prime Gen" HorizontalAlignment="Left" Margin="16,65,0,0" VerticalAlignment="Top" Width="120" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="16"/>
        <TextBox x:Name="primeGenOutputBox" HorizontalAlignment="Left" Margin="16,155,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="197" IsReadOnly="True"/>
        <Label Content="Fib Gen" HorizontalAlignment="Left" Margin="178,62,0,0" VerticalAlignment="Top" Width="120" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="16"/>
        <Button x:Name="fibGenStartBtn" Content="Start" HorizontalAlignment="Left" Margin="178,98,0,0" VerticalAlignment="Top" Width="50" Click="fibGenStartBtn_Click" Height="20"/>
        <TextBox x:Name="fibGenOutputBox" HorizontalAlignment="Left" Margin="178,155,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="197" IsReadOnly="True"/>
        <Button x:Name="primeGenStopBtn" Content="Stop" HorizontalAlignment="Left" Margin="16,123,0,0" VerticalAlignment="Top" Width="50" Click="primeGenStopBtn_Click" Height="20"/>
        <Button x:Name="fibGenStopBtn" Content="Stop" HorizontalAlignment="Left" Margin="178,123,0,0" VerticalAlignment="Top" Width="50" Click="fibGenStopBtn_Click" Height="20"/>
        <Button x:Name="primeGenPauseBtn" Content="Pause" HorizontalAlignment="Left" Margin="86,98,0,0" VerticalAlignment="Top" Width="50" Click="primeGenPauseBtn_Click" Height="20"/>
        <Button x:Name="primeGenResumeBtn" Content="Resume" HorizontalAlignment="Left" Margin="86,123,0,0" VerticalAlignment="Top" Width="50" Click="primeGenResumeBtn_Click" Height="20"/>
        <Button x:Name="fibGenPauseBtn" Content="Pause" HorizontalAlignment="Left" Margin="248,98,0,0" VerticalAlignment="Top" Width="50" Click="fibGenPauseBtn_Click" Height="20"/>
        <Button x:Name="fibGenResumeBtn" Content="Resume" HorizontalAlignment="Left" Margin="248,123,0,0" VerticalAlignment="Top" Width="50" Click="fibGenResumeBtn_Click" Height="20"/>

    </Grid>
</Window>
